{
  "name": "filament",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/Graph.js",
  "typings": "dist/Graph.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "clean && tsc -p .",
    "test": "jest --watch --noStackTrace",
    "test:verbose": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "tsc -p ."
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.9",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.4",
    "ts-jest": "^22.4.6"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
